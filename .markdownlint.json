  "MD030": { "ul_single": 1, "ol_single": 1, "ul_multi": 1, "ol_multi": 1 },
  "MD033": { "allowed_elements": ["a", "img", "br", "details", "summary"] },
  "MD036": false,
  "MD041": { "front_matter_title": "" },
